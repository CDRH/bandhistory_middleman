<html>
  <head>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23044707-13"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-23044707-13');
    </script>

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title %></title>

    <!-- favicon includes -->
    <%= favicon_tag 'favicons/apple-touch-icon.png', rel: "apple-touch-icon", sizes: "180x180" %>
    <%= favicon_tag 'favicons/favicon-32x32.png', rel: "icon", sizes: "32x32", type: "image/png" %>
    <%= favicon_tag 'favicons/favicon-16x16.png', rel: "icon", sizes: "16x16", type: "image/png" %>
    <%= favicon_tag 'favicons/favicon.ico', rel: "icon" %>

    <!-- stylesheets -->
    <link href="https://fonts.googleapis.com/css?family=Cinzel|Cutive&display=swap" rel="stylesheet">
    <%= stylesheet_link_tag "custom-bootstrap" %>
    <%= stylesheet_link_tag "site" %>

    <!-- [if lt IE 10]>
      <style>
        .banner-header, .bg-footer {
          background-color: #000;
        }
      </style>
    <![endif]-->
  </head>

  <body id="page-top" class="<%= page_classes %>">
    <header role="banner">
      <div class="container banner-header">
        <nav class="navbar navbar-expand-lg navbar-dark">
          <h1>
            <%= link_to "Cornhusker Marching Band History", "/index.html", class: "navbar-brand" %>
          </h1>
          <ul class="navbar-nav mr-auto">

            <%= partial "layouts/nav-item", locals: {
              label: "Sights & Sounds", path: "multimedia" } %>

            <%= partial "layouts/nav-item", locals: {
              label: "Timeline", path: "timeline" } %>

            <%= partial "layouts/nav-item", locals: {
              label: "Stories", path: "stories" } %>

            <%= partial "layouts/nav-item", locals: {
              label: "About", path: "about" } %>

          </ul>
        </nav>
      </div>
    </header>

    <div class="container">
      <%= yield %>
    </div>

    <footer>
      <div class="container">
        <div class="card text-white bg-footer">
          <div class="card-body footer">
            <p class="logo_container card-text">
              <div class="row">
                <div class="col-md-4">
                  <a href="https://unl.edu" alt="University of Nebraska-Lincoln"><%= image_tag "/images/content/template/NUNL.png", alt: "UNL Logo" %></a>
                </div>
                <div class="col-md-8">
                  Support provided by <a href="https://cdrh.unl.edu/">Center for Digital Research in the Humanities</a>, <a href="https://libraries.unl.edu/archives-special-collections">Archives & Special Collections</a>, the <a href="https://www.unl.edu/band/welcome">University Bands</a>, and the <a href="https://www.unl.edu/">University of Nebraska-Lincoln</a>
                </div>
              </div>
            </p>
          </div>
        </div>
      </div>
    </footer>

    <%= javascript_include_tag "jquery.min" %>
    <%= javascript_include_tag "bootstrap-4.0.0.min" %>
    <%= javascript_include_tag "site" %>

  </body>
</html>
